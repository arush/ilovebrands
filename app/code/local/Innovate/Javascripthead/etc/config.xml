      <jobs>
          <get_javascripthead_view>
              <schedule><cron_expr>*/2 * * * *</cron_expr></schedule>
              <run><model>javascripthead/observer::cacheFeeds</model></run>
          </get_javascripthead_view>
      </jobs>
  </crontab>-->
</config>