.sales-order-history .pager .limiter label { padding-right:15px; }
.pager .pages { text-align:center; color:#fff; line-height:20px; float:right; width:130px; }
.pager .pages a { color:#fff; text-decoration:underline; }
.pager .pages a:hover { color:#8c8c8c; text-decoration:none; }
