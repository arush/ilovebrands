#product_sendtofriend_form .form-list label, #product_sendtofriend_form  input, .product-gift-card .form-list label, .product-gift-card .form-list input { color: #C1C1C1; }
.form-list span.label { font-weight:normal; }
.form-list .input-box { float:left; /* width:256px; */ }
.customer-account-create .form-list .input-box, .enterprise-invitation-customer-account-create .form-list .input-box { width: 230px; }
.form-list .input-box a { font-size:11px; margin-top:4px; }
.form-list input.input-text, .form-list textarea {
	float:left;
	width:374px;
	vertical-align:top;
	background-color:#1B1A1A;
