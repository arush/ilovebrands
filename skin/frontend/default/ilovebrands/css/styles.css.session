/* Sorter */
.sorter { float:left; width:130px; font-size:11px; margin:0 0 0; padding:4px 15px 0 10px; color:#fff; }
.sorter p { margin:0; }
.sorter select { padding:0; }
.sorter .view-mode { float:left; line-height:20px; white-space:nowrap; }
