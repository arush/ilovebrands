.sub-title a { font-size:11px; font-weight:normal; text-transform:lowercase; }
.section-title { font-size:14px; font-weight:normal; color:#151515; margin:0 0 5px; }

/* Pager */
.pager { float:right; /*width:190px;*/ margin:15px 0; font-size:11px; padding:4px 10px 0 0; color:#fff; }
.pager p { margin:0; }
.pager select { padding:0; display:inline-block; }
.pager .amount { float:left; line-height:20px; }
.pager .limiter {line-height:20px; float:right; width:90px; }
